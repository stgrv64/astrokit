alias alert='notify-send --urgency=low -i "$([ $? = 0 ] && echo terminal || echo error)" "$(history|tail -n1|sed -e '\''s/^\s*[0-9]\+\s*//;s/[;&|]\s*alert$//'\'')"'
alias arp-local='sudo arp-scan --localnet'
alias arp-rpi-locate='sudo arp-scan --localnet | grep Rasp'
alias arp_scan_hon_hai_precision='sudo arp-scan --localnet | grep -i "Hon Hai Precision"'
alias arp_scan_hon_hai_precision_ip='sudo arp-scan --localnet | grep -i "Hon Hai Precision" | awk '\''{print $1}'\'''
alias astroclone='git clone -b develop  ssh://git@github.com/stgrv64/astrokit.git'
alias bitbake-e-core-image-minimal='bitbake -e core-image-minimal | tee -a ./bitbake-e.core-image-minimal'
alias bitbake-e-core-image-rt='bitbake -e core-image-rt | tee -a ./bitbake-e.core-image-rt'
alias bitbake-layers-show-recipes='bitbake-layers show-recipes | tee -a ./bitbake-layers.show-recipes'
alias cat-bitbake-e.core-image-minimal='cat bitbake-e.core-image-minimal'
alias cdastrokit='cd   /home/stef/git/astrokit'
alias cdprocedures='cd /home/stef/git/procedures'
alias cdrttests='cd    /home/stef/git/rt-tests'
alias cdscripts='cd    /home/stef/git/scripts'
alias cdssd='cd        /media/stef/ssd'
alias cdssd256='cd     /media/stef/ssd256'
alias cdyoasdunf='cd   /media/stef/ssd/yocto-astrokit-dunfell'
alias egrep='egrep --color=auto'
alias fgrep='fgrep --color=auto'
alias findallc='find . -name $f;echo "=====";find . -type f -exec grep -l $f {} \;'
alias findallcc='find . -type f -exec grep $f {} \;'
alias findallccc='for file in $(find . -type f); do grep -l $f $file ; grep $f $file ; done ;'
alias findinc='find . -name $f;echo "=====";find . -type f -name "*.c" -exec grep -l $f {} \;;echo "=====";find . -type f -name "*.h" -exec grep -l $f {} \;;'
alias findincc='find . -type f -name "*.c" -exec grep $f {} \;;echo "=====";find . -type f -name "*.h" -exec grep $f {} \;;'
alias findinccc='for file in $(find . -type f -name "*.c"); do grep -l $f $file ; grep $f $file ; done ;'
alias findincch='for file in $(find . -type f -name "*.h"); do grep -l $f $file ; grep $f $file ; done ;'
alias git-meta-pull='for met in $(find . -type d -name "meta-*"); do echo $met; git -C $met pull ; done ;'
alias grep='grep --color=auto'
alias l='ls -CF'
alias la='ls -A'
alias ll='ls -alF'
alias ls='ls --color=auto'
alias sd-df='df -h | grep mmc'
alias sd-echo='echo "---------------------------"'
alias sd-echo-cd-bootfs='echo "cd /media/stef/bootfs"'
alias sd-echo-cd-rootfs='echo "cd /media/stef/rootfs"'
alias sd-ls='ls /media/stef/bootfs ; ls /media/stef/rootfs'
alias sd-mount=' sd-df;sd-echo;sd-mount-p1-bootfs;sd-echo;sd-mount-p2-rootfs;sd-echo;sd-df;sd-echo;sd-ls;sd-echo-cd-bootfs;sd-echo-cd-rootfs'
alias sd-mount-p1-bootfs='sudo umount /dev/mmcblk0p1 ; sudo umount /media/stef/bootfs ; sudo mount /dev/mmcblk0p1 /media/stef/bootfs'
alias sd-mount-p2-rootfs='sudo umount /dev/mmcblk0p2 ; sudo umount /media/stef/rootfs ; sudo mount /dev/mmcblk0p2 /media/stef/rootfs'
alias sd-umount-all='sudo umount /dev/mmcblk* ; sudo umount /media/stef/boo* ; sudo umount /media/stef/roo*'
alias sd-umount-p1-bootfs='sudo umount /dev/mmcblk0p1 ; sudo umount /media/stef/bootfs'
alias sd-umount-p1p2='sd-umount-p1-bootfs ; sd-umount-p2-rootfs ; sd-df ; df'
alias sd-umount-p2-rootfs='sudo umount /dev/mmcblk0p2 ; sudo umount /media/stef/rootfs'
alias ssh_dell_lent='ssh stef@$ipdelllent'
